    "# 创建全是1的数组\n",
    "a = np.ones((2, 3))\n",
    "print('ones{0}{1}{2}'.format('\\n', a, '\\n'))\n",
    "\n",
    "# 创建1全是0的数组\n",
    "a = np.zeros((2, 3))\n",
    "print('zeros{0}{1}{2}'.format('\\n', a, '\\n'))\n",
    "\n",
    "# 创建对内存不进行初始化的数组\n",
    "a = np.empty((2, 3))\n",
    "print('empty{0}{1}{2}'.format('\\n', a, '\\n'))\n",
    "\n",
    "# 创建含有指定元素的数字\n",
