    "        return new_price\n",
    "\n",
    "    @price.setter\n",
    "    def price(self,value):\n",
    "        self.original_price = value\n",
    "\n",
    "    @price.deleter\n",
    "    def price(self):\n",
    "        del self.original_price\n",
